{
    "name": "fowitech/laravel-localization",
    "description": "Easy localization for Laravel",
    "keywords": ["localization", "laravel", "php"],
    "homepage": "https://github.com/fowitech/laravel-localization",
    "license": "MIT",
    "authors": [
        {
            "name": "Fowitech",
            "email": "info@fowitech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/auth": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/container": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.4|^10.1|^11.5"
    },
    "suggest": {
        "ext-intl": "*"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "Fowitech\\Localization\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fowitech\\Localization\\LocalizationServiceProvider"
            ],
            "aliases": {
                "Localization": "Fowitech\\Localization\\Facades\\Localization"
            }
        }
    },
    "minimum-stability": "dev"
}
